#include<stdio.h>

int main()
{
    int a, b, c;
    scanf("%d%d%d",&a,&b,&c);
    
    if(a<b && a<c){
    printf("%d\n",a);
    }
    if(b<a && b<c){
    printf("%d\n",b);
    }
    if(c<a && c<b){
    printf("%d\n",c);
    }
    
    
    if((a>b && a<c) || (a>c && a<b)){
    printf("%d\n",a);
    }
    if((b>a && b<c) || (b>c && b<a)){
    printf("%d\n",b);
    }
    if((c>a && c<b) || (c>b && c<a)){
    printf("%d\n",c);
    }
    
    
    if(a>b && a>c){
    printf("%d\n",a);
    }
    if(b>a && b>c){
    printf("%d\n",b);
    }
    if(c>a && c>b){
    printf("%d\n",c);
    }
    printf("\n");
    
    printf("%d\n%d\n%d\n",a,b,c);
    return 0;
}
